{
  "type": "boolean",
  "title": {
    "en": "Vacuum cleaner dust bin missing",
    "nl": "Stofzuigerbak ontbreekt"
  },
  "desc": {
    "en": "True when the vacuum cleaner's dust bin is missing."
  },
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "alarm_vacuumcleaner_bin_missing_true",
        "highlight": true,
        "title": {
          "en": "Vacuum cleaner dust bin missing"
        }
      },
      {
        "id": "alarm_vacuumcleaner_bin_missing_false",
        "title": {
          "en": "Vacuum cleaner dust bin found"
        }
      }
    ],
    "conditions": [
      {
        "id": "alarm_vacuumcleaner_bin_missing",
        "title": {
          "en": "The vacuum cleaner's dust bin is !{{missing|found}}"
        }
      }
    ]
  }
}