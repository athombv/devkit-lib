{
  "type": "number",
  "title": {
    "en": "pH level",
    "nl": "pH-waarde"
  },
  "desc": {
    "en": "The pH level of a aqueous solution.",
    "nl": "De pH-waarde van een oplossing."
  },
  "insights": true,
  "chartType": "spline",
  "decimals": 2,
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "measure_ph_changed",
        "title": {
          "en": "The pH level changed",
          "nl": "De pH-waarde is veranderd"
        },
        "tokens": [
          {
            "name": "$id",
            "title": "$title",
            "type": "$type",
            "example": 2
          }
        ]
      }
    ]
  }
}