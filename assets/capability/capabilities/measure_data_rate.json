{
  "type": "number",
  "title": {
    "en": "Data Rate",
    "nl": "Gegevenssnelheid"
  },
  "units": {
    "en": "b/s",
    "nl": "b/s"
  },
  "desc": {
    "en": "The data rate in bits per second.",
    "nl": "De gegevenssnelheid in bits per seconde."
  },
  "insights": true,
  "chartType": "spline",
  "decimals": 0,
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "measure_data_rate_changed",
        "title": {
          "en": "The data rate changed",
          "nl": "De gegevenssnelheid is veranderd"
        },
        "tokens": [
          {
            "name": "$id",
            "title": "$title",
            "type": "$type",
            "example": 200
          }
        ]
      }
    ]
  }
}