{
  "title": {
    "en": "Window coverings tilt set",
    "nl": "Raambekledingkanteling instellen",
    "de": "Jalousien-Kippstellung setzen",
    "fr": "Définir l'inclinaison des stores",
    "it": "Imposta l'inclinazione delle tapparelle",
    "sv": "Ställ in lutning för fönsterskydd",
    "no": "Innstill helning for vindusbeskyttelse",
    "es": "Configurar la inclinación de los cobertores de ventanas",
    "da": "Indstil hældning for vinduesdække",
    "ru": "Угол наклона оконных навесов установлен",
    "pl": "Ustawienie osłon okiennych",
    "ko": "창문 덮개 틸트 설정"
  },
  "type": "number",
  "min": 0,
  "max": 1,
  "units": {
    "en": "%",
    "ko": "%",
    "no": "%"
  },
  "decimals": 2,
  "getable": true,
  "setable": true,
  "uiComponent": "slider",
  "$flow": {
    "triggers": [
      {
        "id": "windowcoverings_tilt_set_changed",
        "title": {
          "en": "The tilt position changed"
        },
        "tokens": [
          {
            "name": "windowcoverings_tilt_set",
            "type": "number",
            "title": {
              "en": "Position"
            },
            "example": 0.5
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "windowcoverings_tilt_set",
        "highlight": true,
        "title": {
          "en": "Set the tilt position to"
        },
        "hint": {
          "en": "Change the tilt position to open (100%), closed (0%), or a value in between."
        },
        "args": [
          {
            "name": "windowcoverings_tilt_set",
            "type": "range",
            "min": 0,
            "max": 1,
            "step": 0.01,
            "value": 0.5,
            "label": "%",
            "labelMultiplier": 100,
            "labelDecimals": 0
          }
        ]
      }
    ]
  }
}