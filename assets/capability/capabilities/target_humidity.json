{
  "type": "number",
  "title": {
    "en": "Target Humidity",
    "nl": "Doelvochtigheid"
  },
  "units": {
    "en": "%",
    "nl": "%"
  },
  "desc": {
    "en": "The target humidity level in percent.",
    "nl": "Het doelvochtigheidsniveau in procent."
  },
  "min": 0,
  "max": 1,
  "decimals": 2,
  "insights": true,
  "getable": true,
  "setable": true,
  "uiComponent": "slider",
  "$flow": {
    "triggers": [
      {
        "id": "target_humidity_changed",
        "title": {
          "en": "The target humidity changed",
          "nl": "De doelvochtigheid is gewijzigd"
        },
        "tokens": [
          {
            "name": "$id",
            "title": "$title",
            "type": "$type",
            "example": 0.5
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "set_target_humidity",
        "title": {
          "en": "Set the humidity",
          "nl": "Stel de vochtigheid in"
        },
        "args": [
          {
            "name": "target_humidity",
            "type": "range",
            "min": 0,
            "max": 1,
            "step": 0.01,
            "value": 0.5,
            "label": "%",
            "labelMultiplier": 100,
            "labelDecimals": 0
          }
        ]
      }
    ]
  },
  "minCompatibility": "12.1.0"
}