{
  "type": "boolean",
  "title": {
    "en": "Oscillating",
    "nl": "Oscilleren"
  },
  "desc": {
    "en": "Enable or disable oscillation.",
    "nl": "Schakel oscilleren in of uit."
  },
  "insights": true,
  "insightsTitleTrue": {
    "en": "Oscillation enabled",
    "nl": "Oscilleren ingeschakeld"
  },
  "insightsTitleFalse": {
    "en": "Oscillation disabled",
    "nl": "Oscilleren uitgeschakeld"
  },
  "getable": true,
  "setable": true,
  "uiComponent": "toggle",
  "$flow": {
    "triggers": [
      {
        "id": "oscillating_true",
        "highlight": true,
        "title": {
          "en": "Oscillation enabled",
          "nl": "Oscilleren ingeschakeld"
        }
      },
      {
        "id": "oscillating_false",
        "title": {
          "en": "Oscillation disabled",
          "nl": "Oscilleren uitgeschakeld"
        }
      }
    ],
    "conditions": [
      {
        "id": "oscillating",
        "title": {
          "en": "Oscillation is !{{enabled|disabled}}",
          "nl": "Oscilleren is !{{ingeschakeld|uitgeschakeld}}"
        }
      }
    ],
    "actions": [
      {
        "id": "enable_oscillating",
        "title": {
          "en": "Enable Oscillation",
          "nl": "Zet oscilleren aan"
        }
      },
      {
        "id": "disable_oscillating",
        "title": {
          "en": "Disable Oscillation",
          "nl": "Zet oscilleren uit"
        }
      },
      {
        "id": "toggle_oscillating",
        "title": {
          "en": "Toggle Oscillation on or off",
          "nl": "Schakel oscilleren aan of uit"
        }
      }
    ]
  }
}