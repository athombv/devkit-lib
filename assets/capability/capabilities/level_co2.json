{
  "type": "enum",
  "title": {
    "en": "CO2 Level",
    "nl": "CO2-niveau"
  },
  "desc": {
    "en": "The level of CO2 in the air.",
    "nl": "Het niveau van CO2 in de lucht."
  },
  "values": [
    {
      "id": "low",
      "title": {
        "en": "Low",
        "nl": "Laag"
      }
    },
    {
      "id": "medium",
      "title": {
        "en": "Medium",
        "nl": "Middel"
      }
    },
    {
      "id": "high",
      "title": {
        "en": "High",
        "nl": "Hoog"
      }
    },
    {
      "id": "critical",
      "title": {
        "en": "Critical",
        "nl": "Kritiek"
      }
    }
  ],
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "level_co2_changed",
        "highlight": true,
        "title": {
          "en": "The CO2 level changed to",
          "nl": "Het CO2-niveau is veranderd naar"
        },
        "args": [
          {
            "name": "level",
            "type": "dropdown",
            "values": "$values"
          }
        ]
      }
    ],
    "conditions": [
      {
        "id": "level_co2_is",
        "title": {
          "en": "The CO2 level !{{is|is not}}",
          "nl": "Het CO2-niveau !{{is|is niet}}"
        },
        "args": [
          {
            "name": "level",
            "type": "dropdown",
            "values": "$values"
          }
        ]
      }
    ]
  }
}