{
  "type": "enum",
  "title": {
    "en": "Air quality level",
    "nl": "Luchtkwaliteitsniveau"
  },
  "desc": {
    "en": "The air quality index represented as a level.",
    "nl": "De luchtkwaliteitsindex weergegeven als niveau."
  },
  "values": [
    {
      "id": "good",
      "title": {
        "en": "Good",
        "nl": "Goed"
      }
    },
    {
      "id": "poor",
      "title": {
        "en": "Poor",
        "nl": "Slecht"
      }
    },
    {
      "id": "very_poor",
      "title": {
        "en": "Very Poor",
        "nl": "Zeer Slecht"
      }
    },
    {
      "id": "extremely_poor",
      "title": {
        "en": "Extremely Poor",
        "nl": "Extreem Slecht"
      }
    }
  ],
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "level_aqi_changed",
        "highlight": true,
        "title": {
          "en": "The air quality level changed to",
          "nl": "Het luchtkwaliteitsniveau is veranderd naar"
        },
        "args": [
          {
            "name": "level",
            "type": "dropdown",
            "values": "$values"
          }
        ]
      }
    ],
    "conditions": [
      {
        "id": "level_aqi_is",
        "title": {
          "en": "The air quality !{{is|is not}}",
          "nl": "De luchtkwaliteit !{{is|is niet}}"
        },
        "args": [
          {
            "name": "level",
            "type": "dropdown",
            "values": "$values"
          }
        ]
      }
    ]
  },
  "minCompatibility": "12.1.0"
}